<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.17.xsd">

<changeSet id="seed-products" author="you">
<preConditions onFail="MARK_RAN">
<not>
<sqlCheck expectedResult="0">
select count(*) from product
</sqlCheck>
</not>
</preConditions>

<insert tableName="product">
<column name="name" value="Phone X"/>
<column name="price" valueNumeric="299.90"/>
<column name="explanation" value="Entry smartphone"/>
<column name="category_id"
valueComputed="(select id from category where name='Electronics')"/>
</insert>

<insert tableName="product">
<column name="name" value="Organic Apple (1kg)"/>
<column name="price" valueNumeric="2.99"/>
<column name="explanation" value="Fresh apples"/>
<column name="category_id"
valueComputed="(select id from category where name='Groceries')"/>
</insert>
</changeSet>

</databaseChangeLog>
